{
  "name": "docs",
  "private": true,
  "scripts": {
    "prepare": "husky install",
    "dev": "vitepress dev src --open",
    "build": "vitepress build src",
    "serve": "vitepress serve src",
    "format": "prettier --write src/**",
    "lint-staged": "lint-staged"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,md}": [
      "prettier --ignore-path ./.prettierignore --write "
    ]
  },
  "author": "Mix Space Team",
  "license": "CC 3.0",
  "dependencies": {
    "@types/markdown-it": "*",
    "@types/prismjs": "*",
    "flexsearch": "^0.7.31",
    "@vue/theme": "2.2.1",
    "@vueuse/core": "9.13.0",
    "@vueuse/head": "1.1.23",
    "markdown-it": "^13.0.1",
    "markdown-it-checkbox": "1.1.0",
    "prismjs": "^1.29.0",
    "segment": "^0.1.3",
    "shiki": "0.14.1",
    "vitepress-plugin-search": "1.0.4-alpha.20",
    "vite": "4.2.1",
    "vitepress": "1.0.0-alpha.61",
    "vue": "3.2.47"
  },
  "pnpm": {
    "peerDependencyRules": {
      "ignoreMissing": [
        "@algolia/client-search",
        "react",
        "react-dom",
        "@types/react"
      ]
    }
  },
  "devDependencies": {
    "@types/node": "18.15.8",
    "husky": "8.0.3",
    "lint-staged": "13.2.0",
    "prettier": "2.8.7",
    "terser": "5.16.8",
    "vite-plugin-windicss": "1.8.10",
    "windicss": "3.5.6"
  }
}