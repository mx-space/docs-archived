{
  "name": "docs",
  "private": true,
  "scripts": {
    "prepare": "husky install",
    "dev": "vitepress dev src --open",
    "build": "vitepress build src",
    "serve": "vitepress serve src",
    "format": "prettier --write src/**",
    "lint-staged": "lint-staged"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,md}": [
      "prettier --ignore-path ./.prettierignore --write "
    ]
  },
  "author": "Mix Space Term",
  "license": "CC 3.0",
  "dependencies": {
    "@types/markdown-it": "12.2.3",
    "@types/prismjs": "1.26.0",
    "@vue/theme": "1.0.4",
    "@vueuse/core": "9.3.0",
    "@vueuse/head": "0.9.8",
    "markdown-it": "13.0.1",
    "markdown-it-checkbox": "1.1.0",
    "prismjs": "1.29.0",
    "shiki": "0.11.1",
    "vite": "3.0.8",
    "vitepress": "1.0.0-alpha.21",
    "vue": "3.2.40"
  },
  "pnpm": {
    "peerDependencyRules": {
      "ignoreMissing": [
        "@algolia/client-search",
        "react",
        "react-dom",
        "@types/react"
      ]
    }
  },
  "devDependencies": {
    "@types/node": "17.0.23",
    "husky": "8.0.1",
    "lint-staged": "13.0.3",
    "prettier": "2.7.1",
    "terser": "5.15.1",
    "vite-plugin-windicss": "1.8.8",
    "windicss": "3.5.6"
  }
}