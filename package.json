{
  "name": "docs",
  "private": true,
  "scripts": {
    "prepare": "husky install",
    "dev": "vitepress dev src --open",
    "build": "vitepress build src --mpa",
    "serve": "vitepress serve src",
    "format": "prettier --write src/**"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,md}": [
      "prettier --ignore-path ./.prettierignore --write "
    ]
  },
  "author": "Mix Space Term",
  "license": "CC 3.0",
  "dependencies": {
    "@vue/theme": "1.0.1",
    "shiki": "0.10.1",
    "vite": "2.9.1",
    "vitepress": "0.22.3",
    "vue": "3.2.31"
  },
  "pnpm": {
    "peerDependencyRules": {
      "ignoreMissing": [
        "@algolia/client-search",
        "react",
        "react-dom",
        "@types/react"
      ]
    }
  },
  "devDependencies": {
    "@types/node": "17.0.23",
    "husky": "7.0.4",
    "lint-staged": "12.3.7",
    "prettier": "2.6.2"
  }
}